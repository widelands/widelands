msgid "%1% hour and %2%"
msgid_plural "%1% hours and %2%"
msgstr[0] "%1% uur en %2%"
msgstr[1] "%1% uur en %2%"

#. TRANSLATORS: Context: 'The game will end in 2 hours.'
#: ../../data/scripting/win_conditions/win_condition_functions.lua:308
msgid "%1% hour"
