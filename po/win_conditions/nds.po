msgid "%i tree"
msgid_plural "%i trees"
msgstr[0] "%i Boom"
msgstr[1] "%i Bomen"

#. TRANSLATORS: %1$s = player name, %2$s = x tree(s)
#: ../../data/scripting/win_conditions/wood_gnome.lua:67
msgid "%1$s has %2$s at the moment."
msgstr "%1$s hett jüüst %2$s."

#. TRANSLATORS: %1$s = player name, %2$s = x tree(s)
#: ../../data/scripting/win_conditions/wood_gnome.lua:88
msgid "%1$s had %2$s."
msgstr "%1$s harr %2$s."

#. TRANSLATORS: %1$s = player name, %2$s = x tree(s)
#: ../../data/scripting/win_conditions/wood_gnome.lua:94
msgid "The winner is %1$s with %2$s."
msgstr "%1$s hett mit %2$s wunnen."
