msgid "Artifact Lost"
msgstr "Artefacto perdido"

#: ../../data/scripting/win_conditions/artifacts.lua:90
msgid "One of your team’s artifacts was stolen by an enemy."
msgstr "Uno de los artefactos de tu equipo fue robado por el enemigo."

#: ../../data/scripting/win_conditions/artifacts.lua:91
msgid "One of your artifacts was stolen by an enemy."
msgstr "Uno de los artefactos tuyos fue robado por el enemigo."

#. TRANSLATORS: Keep this as short as possible.
#: ../../data/scripting/win_conditions/artifacts.lua:95
msgid "Artifact Conquered"
msgstr "Artefacto Conquistado"

#: ../../data/scripting/win_conditions/artifacts.lua:96
msgid "Your team stole an artifact from an enemy."
msgstr "Tu equipo robó un artefacto del enemigo."

#: ../../data/scripting/win_conditions/artifacts.lua:97
msgid "You stole an artifact from an enemy."
msgstr "Robaste un artefacto del enemigo."

#: ../../data/scripting/win_conditions/artifacts.lua:180
msgid "Overview:"
msgstr "Vista Previa:"

#: ../../data/scripting/win_conditions/artifacts.lua:183
#: ../../data/scripting/win_conditions/artifacts.lua:196
#, lua-format
msgid "%i artifact"
msgid_plural "%i artifacts"
