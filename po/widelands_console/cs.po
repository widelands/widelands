#. * TRANSLATORS: %s = version information
#: ../../src/wlapplication_messages.cc:38
#, c-format
msgid "This is Widelands Build %s"
msgstr "Toto je Widelands Build %s"

#: ../../src/wlapplication_messages.cc:43
msgid "Usage: widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "Použití: widelands <přepínač0>=<hodnota0> ... <přepínačN>=<hodnotaN>"

#: ../../src/wlapplication_messages.cc:44
msgid "Options:"
msgstr "Volby:"

#: ../../src/wlapplication_messages.cc:45
msgid " --<config-entry-name>=value overwrites any config file setting"
msgstr " --<config-entry-name>=hodnota přepíše jakékoli nastavení v configu"

#: ../../src/wlapplication_messages.cc:47
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr " --datadir=DIRNAME    Použij určený adresář pro Widelands datové soubory"

#: ../../src/wlapplication_messages.cc:50
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr " --homedir=DIRNAME   Použij určený adresář pro Widelands config, uložené hry a záznamy"

#: ../../src/wlapplication_messages.cc:54
msgid "                      Default is ~/.widelands"
msgstr "Standardní je ~/.widelands"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:60
msgid ""
" --coredump=[true|false]\n"
"                      Generates a core dump on segfaults instead of\n"
"                      using the SDL"
msgstr " --coredump=[true|false]\n                      Generuje core dump při segfault chybě místo použití SDL"

#: ../../src/wlapplication_messages.cc:64
msgid ""
" --language=[de_DE|sv_SE|...]\n"
"                      The locale to use."
msgstr " --language=[de_DE|sv_SE|...] Použité nastavení locale"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain.
#. * TRANSLATORS: A syncstream is a synchronization stream. Syncstreams are
#. used in
#. multiplayer
#. * TRANSLATORS: games to make sure that there is no mismatch between the
#. players.
#: ../../src/wlapplication_messages.cc:73
msgid ""
" --write_syncstreams=[true|false]\n"
"                      Create syncstream dump files to help debug network games."
msgstr " --write_syncstreams=[true|false]\n                      Vytvořit syncstream dump soubory pro debugování síťových her."

#: ../../src/wlapplication_messages.cc:76
msgid " --autosave=[...]     Automatically save each n minutes"
msgstr " --autosave=[...]  Automaticky ukládá každých n minut"

#: ../../src/wlapplication_messages.cc:77
msgid ""
" --rolling_autosave=[...]\n"
"                      Use this many files for rolling autosaves"
msgstr " --rolling_autosave=[...] Použij uvedený počet souborů pro postupné autosave"

#: ../../src/wlapplication_messages.cc:82
msgid "Sound options:"
msgstr "Volby zvuku:"

#: ../../src/wlapplication_messages.cc:83
msgid " --nosound            Starts the game with sound disabled."
msgstr " --nosound            Spustí hru bez zvuku."

#: ../../src/wlapplication_messages.cc:84
msgid " --disable_fx         Disable sound effects."
msgstr " --disable_fx         Vypne zvukové efekty."

#: ../../src/wlapplication_messages.cc:85
msgid " --disable_music      Disable music."
msgstr " --disable_music      Vypne hudbu."

#: ../../src/wlapplication_messages.cc:87
msgid " --nozip              Do not save files as binary zip archives."
msgstr " --nozip              Neukládat soubory jako binární zip archivy."

#: ../../src/wlapplication_messages.cc:89
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr " --editor             Přímo spustí editor Widelands.\n                      Můžeš přidat =SOUBOR který přímo nahraje\n                      mapu SOUBOR do editoru."

#: ../../src/wlapplication_messages.cc:93
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr " --scenario=SOUBOR    Přímo spustí mapu SOUBOR jako mapu\n                      scénáře."

#: ../../src/wlapplication_messages.cc:96
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=SOUBOR    Přímo nahraje uloženou hru SOUBOR."

#: ../../src/wlapplication_messages.cc:97
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr " --script=FILENAME    Spusti uvedený Lua skript po inicializaci. Platné pouze s volbami --scenario, --loadgame, nebo --editor."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:103
msgid ""
" --auto_roadbuild_mode=[true|false]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr " --auto_roadbuild_mode=[true|false] Určení, zda spustit mód obnovy automaticky po umístění vlajky nenapojené na síť cest"

#: ../../src/wlapplication_messages.cc:109
msgid "Graphic options:"
msgstr "Grafické volby:"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:113
msgid ""
" --fullscreen=[true|false]\n"
"                      Whether to use the whole display for the\n"
"                      game screen."
msgstr " --fullscreen=[true|false] Zda hru spustit ve fullscreenu"

#: ../../src/wlapplication_messages.cc:117
msgid " --xres=[...]         Width of the window in pixel."
msgstr " --xres=[...] Šířka okna v pixelech"

#: ../../src/wlapplication_messages.cc:118
msgid " --yres=[...]         Height of the window in pixel."
msgstr " --yres=[...] Výška okna v pixelech"

#: ../../src/wlapplication_messages.cc:123
msgid "Options for the internal window manager:"
msgstr "Volby pro interního správce oken:"

#: ../../src/wlapplication_messages.cc:124
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr " --border_snap_distance=[0 ...]\n                      Přichytit okno k okraji obrazovky jakmile\n                      se okraj okna dostane do této vzdálenosti\n                      od okraje obrazovky."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:132
msgid ""
" --dock_windows_to_edges=[true|false]\n"
"                      Eliminate a window's border towards the\n"
"                      edge of the screen when the edge of the\n"
"                      window is next to the edge of the screen."
msgstr " --dock_windows_to_edges=[true|false] Zruší okraj okna na okraji obrazovky, když je okraj okna blízko okraji obrazovky"

#: ../../src/wlapplication_messages.cc:137
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr " --panel_snap_distance=[0 ...]\n                      Přichytit okno k okraji panelu jakmile se\n                      okraj okna dostane do této vzdálenosti\n                      od okraje jiného panelu."

#: ../../src/wlapplication_messages.cc:142
msgid ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Only move a window to the edge of a panel\n"
"                      if the window is overlapping with the\n"
"                      panel."
msgstr " --snap_windows_only_when_overlapping=[yes|no]\n                      Přichytit okno k okraji panelu pouze\n                      pokud se okno a panel překrývají."

#: ../../src/wlapplication_messages.cc:148
msgid " --verbose            Enable verbose debug messages"
msgstr " --verbose povolí verbose ladící zprávy"

#: ../../src/wlapplication_messages.cc:149
msgid " --help               Show this help"
msgstr " --help               Zobrazí tuto nápovědu"

#: ../../src/wlapplication_messages.cc:150
msgid ""
"Bug reports? Suggestions? Check out the project website:\n"
"        https://launchpad.net/widelands\n"
"\n"
"Hope you enjoy this game!"
msgstr "Nalezli jste nějaký herní bug? Máte nějaké návrhy? Navštivte stránky projektu:\nhttps://launchpad.net/widelands\n\nDoufáme, že si užívate naší hry!"
