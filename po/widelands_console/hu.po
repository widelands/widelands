#. * TRANSLATORS: %s = version information
#: ../../src/wlapplication_messages.cc:37
#, c-format
msgid "This is Widelands Build %s"
msgstr "Ez a Widelans %s verziója"

#: ../../src/wlapplication_messages.cc:42
msgid "Usage: widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "Használat: widelands <beállítás0>=<érték0> ... <beállításN>=<értékN>"

#: ../../src/wlapplication_messages.cc:43
msgid "       widelands <save.wgf>/<replay.wrpl>"
msgstr "           widelands <mentés.wgf>/<visszajátszás.wrpl>"

#: ../../src/wlapplication_messages.cc:44
msgid "Options:"
msgstr "Beállítások:"

#: ../../src/wlapplication_messages.cc:45
msgid " --<config-entry-name>=value overwrites any config file setting"
msgstr " --<beállítás-neve>=érték\n                      A beállító fájlban szereplő beállítás felülírása"

#: ../../src/wlapplication_messages.cc:46
msgid "                      Note: New value will be written to config file"
msgstr "                      Megjegyzés: Az új érték el lesz mentve a beállító fájlba"

#: ../../src/wlapplication_messages.cc:48
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr " --datadir=KÖNYVTÁR   A megadott könyvtár használata a Widelands adatfájlok\n                      betöltéséhez"

#: ../../src/wlapplication_messages.cc:51
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr " --homedir=KÖNYVTÁR   A megadott könyvtár használata a Widelands beállító\n                      fájlok, mentett játékok és visszajátszások tárolására"

#: ../../src/wlapplication_messages.cc:55
msgid "                      Default is ~/.widelands"
msgstr "                      Alapértelmezett: ~/.widelands"

#: ../../src/wlapplication_messages.cc:57
msgid "                      Default is %USERPROFILE%\\.widelands"
msgstr "                      Alapértelmezett: %USERPROFILE%\\.widelands"

#: ../../src/wlapplication_messages.cc:59
msgid ""
" --localedir=DIRNAME  Use specified directory for the widelands\n"
"                      locale files"
msgstr " --localedir=KÖNYVTÁR\n                      A megadott könyvtár használata a Widelands nyelvi\n                      fájlokhoz"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:66
msgid ""
" --coredump=[true|false]\n"
"                      Generates a core dump on segfaults instead of\n"
"                      using the SDL"
msgstr " --coredump=[true|false]\n                      Program állapotának fájlba mentése („core dump”)\n                      memóriakezelési hiba miatti leállás esetén az\n                      „SDL Parachute” használata helyett"

#: ../../src/wlapplication_messages.cc:70
msgid ""
" --language=[de_DE|sv_SE|...]\n"
"                      The locale to use."
msgstr " --language=[de_DE|hu_HU|...]\n                      Használandó nyelv beállítása"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain.
#. * TRANSLATORS: A syncstream is a synchronization stream. Syncstreams are
#. used in
#. multiplayer
#. * TRANSLATORS: games to make sure that there is no mismatch between the
#. players.
#: ../../src/wlapplication_messages.cc:79
msgid ""
" --write_syncstreams=[true|false]\n"
"                      Create syncstream dump files to help debug network games."
msgstr " --write_syncstreams=[true|false]\n                      Szinkronizációs naplófájlok létrehozása a hálózati\n                      játékok hibakeresésének megkönnyítésére (igen/nem)"

#: ../../src/wlapplication_messages.cc:82
msgid " --autosave=[...]     Automatically save each n minutes"
msgstr " --autosave=[...]     Automatikus mentés n percenként"

#: ../../src/wlapplication_messages.cc:83
msgid ""
" --rolling_autosave=[...]\n"
"                      Use this many files for rolling autosaves"
msgstr " --rolling_autosave=[...]\n                      Megőrzendő automatikus mentések számának beállítása"

#: ../../src/wlapplication_messages.cc:86
msgid ""
" --metaserver=[...]\n"
"                      Connect to a different metaserver for internet gaming."
msgstr " --metaserver=[...]\n                      Az internetes játékokhoz használandó metakiszolgáló\n                      beállítása"

#: ../../src/wlapplication_messages.cc:89
msgid ""
" --metaserverport=[...]\n"
"                      Port number of the metaserver for internet gaming."
msgstr " --metaserverport=[...]\n                      Az internetes játékokhoz használt metakiszolgáló\n                      portjának beállítása"

#: ../../src/wlapplication_messages.cc:92
msgid ""
" --addon_server=[...]\n"
"                      Connect to a different github repository\n"
"                      and branch from the add-ons manager."
msgstr " --addon_server=[...]\n                      A bővítménykezelő által használandó github tároló\n                      („repository”) és ág („branch”) beállítása"

#: ../../src/wlapplication_messages.cc:97
msgid " --nosound            Starts the game with sound disabled."
msgstr " --nosound            Hang kikapcsolása"

#: ../../src/wlapplication_messages.cc:99
msgid ""
" --fail-on-lua-error  Force Widelands to crash when a Lua error occurs."
msgstr " --fail-on-lua-error  A Widelands összeomlásra kényszerítése Lua hiba esetén"

#: ../../src/wlapplication_messages.cc:101
msgid " --nozip              Do not save files as binary zip archives."
msgstr " --nozip              A mentések zip formátumba való tömörítésének kikapcsolása"

#: ../../src/wlapplication_messages.cc:103
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr " --editor             A Widelands térképszerkesztő közvetlen indítása\n                      Megadható az indításkor betöltendő térkép is =FÁJLNÉV\n                      hozzáfűzésével"

#: ../../src/wlapplication_messages.cc:107
msgid ""
" --ai_training        Enables AI training mode. See\n"
"                      https://www.widelands.org/wiki/Ai%20Training/\n"
"                      for a full description of the AI training logic."
msgstr " --ai_training        Mesterséges intelligencia oktatómód bekapcsolása\n                      Részletes leírás:\n                      https://www.widelands.org/wiki/Ai%20Training/"

#: ../../src/wlapplication_messages.cc:111
msgid ""
" --auto_speed         In multiplayer games only, this will keep\n"
"                      adjusting the game speed automatically,\n"
"                      depending on FPS. Useful in conjunction with\n"
"                      --ai_training."
msgstr " --auto_speed         A többjátékosos játékokban a játéksebesség automatikus\n                      módosítása a képfrissítési sebesség függvényében\n                      Hasznos lehet az --ai_training kapcsolóval együtt."

#: ../../src/wlapplication_messages.cc:116
msgid ""
" --new_game_from_template=FILENAME\n"
"                      Directly create a new singleplayer game\n"
"                      configured in the given file. An example can\n"
"                      be found in `data/templates/new_game_template`."
msgstr " --new_game_from_template=FÁJLNÉV\n                      Új egyszemélyes játék közvetlen indítása a megadott fájl\n                      alapján. Minta: `data/templates/new_game_template`"

#: ../../src/wlapplication_messages.cc:122
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr " --scenario=FÁJLNÉV   A FÁJLNÉV térkép közvetlen indítása küldetésként"

#: ../../src/wlapplication_messages.cc:125
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=FÁJLNÉV   Játék indítása a FÁJLNÉV mentett állás közvetlen\n                      betöltésével"

#: ../../src/wlapplication_messages.cc:126
msgid " --replay=FILENAME    Directly loads the replay FILENAME."
msgstr " --replay=FÁJLNÉV     A FÁJLNÉV visszajátszás közvetlen indítása"

#: ../../src/wlapplication_messages.cc:127
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr " --script=FÁJLNÉV     A megadott Lua parancsfájl futtatása betöltés után\n                      Csak a --scenario, --loadgame vagy --editor\n                      kapcsolókkal együtt használható."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:133
msgid ""
" --auto_roadbuild_mode=[true|false]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr " --auto_roadbuild_mode=[true|false]\n                      Útépítés mód indítása (igen/nem), úthoz nem\n                      kapcsolódó zászló lehelyezésekor"

#: ../../src/wlapplication_messages.cc:139
msgid "Graphic options:"
msgstr "Grafikai kapcsolók:"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:144
msgid ""
" --fullscreen=[true|false]\n"
"                      Whether to use the whole display for the\n"
"                      game screen."
msgstr " --fullscreen=[true|false]\n                      Teljes képernyős üzemmód használata (igen/nem)"

#: ../../src/wlapplication_messages.cc:148
msgid " --xres=[...]         Width of the window in pixel."
msgstr " --xres=[...]         A programablak szélessége képpontokban"

#: ../../src/wlapplication_messages.cc:149
msgid " --yres=[...]         Height of the window in pixel."
msgstr " --yres=[...]         A programablak magassága képpontokban"

#: ../../src/wlapplication_messages.cc:150
msgid " --maxfps=[5 ...]     Maximal optical framerate of the game."
msgstr " --maxfps=[5 ...]     A játék megengedett legmagasabb képfrissítési sebessége"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:155
msgid "Options for the internal window manager:"
msgstr "Kapcsolók a belső ablakkezelőhöz:"

#: ../../src/wlapplication_messages.cc:156
msgid ""
" --animate_map_panning=[yes|no]\n"
"                      Should automatic map movements be animated."
msgstr " --animate_map_panning=[yes|no]\n                      Automatikus térképmozgások animálása (igen/nem)"

#: ../../src/wlapplication_messages.cc:159
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr " --border_snap_distance=[0 ...]\n                      Ablakok képernyő széléhez igazítása a megadott távolságon\n                      belül"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:167
msgid ""
" --dock_windows_to_edges=[true|false]\n"
"                      Eliminate a window’s border towards the\n"
"                      edge of the screen when the edge of the\n"
"                      window is next to the edge of the screen."
msgstr " --dock_windows_to_edges=[true|false]\n                      Ablakok keretének elrejtése, ahol hozzáérnek a képernyő\n                      széléhez (igen/nem)"

#: ../../src/wlapplication_messages.cc:172
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr " --panel_snap_distance=[0 ...]\n                      Ablakok tálcához igazítása a megadott távolságon belül"

#: ../../src/wlapplication_messages.cc:177
msgid ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Only move a window to the edge of a panel\n"
"                      if the window is overlapping with the\n"
"                      panel."
msgstr " --snap_windows_only_when_overlapping=[yes|no]\n                      Ablakok tálcához igazítása csak átfedés esetén (igen/nem)"

#: ../../src/wlapplication_messages.cc:183
msgid " --verbose            Enable verbose debug messages"
msgstr " --verbose            Bővebb hibakeresési üzenetek"

#: ../../src/wlapplication_messages.cc:184
msgid " --help               Show this help"
msgstr " --help               Ennek a súgónak a megjelenítése"

#: ../../src/wlapplication_messages.cc:186
msgid ""
" <save.wgf>/<replay.wrpl> \n"
"                      Directly loads the given savegame or replay. Useful for\n"
"                      .wgf/.wrpl file extension association. Does not work with\n"
"                      other options. Also see --loadgame/--replay."
msgstr " <mentés.wgf>/<visszajátszás.wrpl>\n                      A megadott mentés vagy visszajátszás közvetlen betöltése\n                      A .wgf/.wrpl fájltípus hozzárendeléséhez használható.\n                      Más beállításokkal együtt nem működik.\n                      Lásd még: --loadgame/--replay"

#: ../../src/wlapplication_messages.cc:192
msgid ""
"Bug reports? Suggestions? Check out the project website:\n"
"        https://www.widelands.org/\n"
"\n"
"Hope you enjoy this game!"
msgstr "Hibabejelentések? Javaslatok? Nézd meg a projekt weblapját:\n        https://www.widelands.org/\n\nReméljük, hogy tetszik a játék!"
