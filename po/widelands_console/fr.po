#: ../../src/wlapplication_messages.cc:51
msgid "Usage:"
msgstr "Usage :"

#: ../../src/wlapplication_messages.cc:51
msgid "widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "widelands <option0>=<value0> ... <optionN>=<valueN>"

