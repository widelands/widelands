#. * TRANSLATORS: %s = version information
#: ../../src/wlapplication_messages.cc:37
#, c-format
msgid "This is Widelands Build %s"
msgstr "Seo Widelands tionndadh %s"

#: ../../src/wlapplication_messages.cc:42
msgid "Usage: widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "Cleachdadh: widelands <roghainn0>=<luach0> ... <roghainnN>=<luachN>"

#: ../../src/wlapplication_messages.cc:43
msgid "Options:"
msgstr "Roghainnean:"

#: ../../src/wlapplication_messages.cc:44
msgid " --<config-entry-name>=value overwrites any config file setting"
msgstr "Sgrìobhaidh --<ainm-na-roghainn>=luach thairis air roghainn\nsam bith san fhaidhle config"

#: ../../src/wlapplication_messages.cc:45
msgid "                      Note: New value will be written to config file"
msgstr "                           An aire: Thèid an luach ùr a sgrìobhadh dhan fhaidhle config"

#: ../../src/wlapplication_messages.cc:47
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr " --datadir=AINM-PASGAIN    Cleachd am pasgan sònraichte airson nam faidhlichean\n                           dàta aig widelands"

#: ../../src/wlapplication_messages.cc:50
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr " --homedir=AINM-PASGAIN    Cleachd am pasgan sònraichte airson nam faidhlichean\n                           rèiteachaidh, geamannan air an sàbhaladh is\n                           ath-chluichean aig widelands"

#: ../../src/wlapplication_messages.cc:54
msgid "                      Default is ~/.widelands"
msgstr "                           'S e ~/.widelands a th' ann a ghnàth"

#: ../../src/wlapplication_messages.cc:56
msgid "                      Default is %USERPROFILE%\\.widelands"
msgstr "                           'S e %USERPROFILE%\\.widelands a th' ann a ghnàth"

#: ../../src/wlapplication_messages.cc:58
msgid ""
" --localedir=DIRNAME  Use specified directory for the widelands\n"
"                      locale files"
msgstr " --localedir=AINM-PASGAIN  Cleachd am pasgan sònraichte airson nam faidhlichean\n                           eadar-theangachaidh aig widelands"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:65
msgid ""
" --coredump=[true|false]\n"
"                      Generates a core dump on segfaults instead of\n"
"                      using the SDL"
msgstr " --coredump=[true|false]   Gin core dump le segfaults an aite SDL\n                           a chleachadh"

#: ../../src/wlapplication_messages.cc:69
msgid ""
" --language=[de_DE|sv_SE|...]\n"
"                      The locale to use."
msgstr " --language=[gd_GB|de_DE|...]\n                           An sgeama ionadail ri chleachdadh."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain.
#. * TRANSLATORS: A syncstream is a synchronization stream. Syncstreams are
#. used in
#. multiplayer
#. * TRANSLATORS: games to make sure that there is no mismatch between the
#. players.
#: ../../src/wlapplication_messages.cc:78
msgid ""
" --write_syncstreams=[true|false]\n"
"                      Create syncstream dump files to help debug network games."
msgstr " --write_syncstreams=[true|false]\n                           Cruthaich faidhlichean sruth sioncronachaidh a chum\n                           geamannan lìonraidh a dhì-bhugachadh."

#: ../../src/wlapplication_messages.cc:81
msgid " --autosave=[...]     Automatically save each n minutes"
msgstr " --autosave=[...]          Sàbhail gu fèin-obrachail gach n mionaid"

#: ../../src/wlapplication_messages.cc:82
msgid ""
" --rolling_autosave=[...]\n"
"                      Use this many files for rolling autosaves"
msgstr " --rolling_autosave=[...]  Cleachd an uiread de dh'fhaidhlichean seo airson\n                           fèin-sàbhaladh car mu char"

#: ../../src/wlapplication_messages.cc:85
msgid ""
" --metaserver=[...]\n"
"                      Connect to a different metaserver for internet gaming."
msgstr " --metaserver=[...]        Dèan ceangal ri meata-fhrithealaiche eile\n                           airson geama eadar-lìn."

#: ../../src/wlapplication_messages.cc:88
msgid ""
" --metaserverport=[...]\n"
"                      Port number of the metaserver for internet gaming."
msgstr " --metaserverport=[...]    Àireamh puirt a’ mheata-fhrithealaiche\n                           airson geama eadar-lìn."

#: ../../src/wlapplication_messages.cc:92
msgid " --nosound            Starts the game with sound disabled."
msgstr " --nosound                 Tòisichidh seo an geama leis an fhuaim à comas."

#: ../../src/wlapplication_messages.cc:94
msgid ""
" --fail-on-lua-error  Force Widelands to crash when a Lua error occurs."
msgstr ""

#: ../../src/wlapplication_messages.cc:96
msgid " --nozip              Do not save files as binary zip archives."
msgstr " --nozip                   Na sàbhail faidhlichean nan tasglannan\n                           bìnearaidh zip."

#: ../../src/wlapplication_messages.cc:98
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr " --editor                  Tòisichidh seo an deasaiche Widelands sa bhad.\n                           'S urrainn dhut =AINM-FAIDHLE a chur ris\n                           gus am mapa air a bheil AINM-FAIDHLE a luchdadh\n                           sa bhad san deasaiche."

#: ../../src/wlapplication_messages.cc:102
msgid ""
" --ai_training        Enables AI training mode. See\n"
"                      https://www.widelands.org/wiki/Ai%20Training/\n"
"                      for a full description of the AI training logic."
msgstr " --ai_training             Cuiridh seo an comas modh trèanaidh na h-inntinne\n                           fuadain. Faic\n                           https://www.widelands.org/wiki/Ai%20Training/\n                           airson mìneachadh slàn air mar a dh'obraicheas\n                           loidsig trèanaidh na h-IF."

#: ../../src/wlapplication_messages.cc:106
msgid ""
" --auto_speed         In multiplayer games only, this will keep\n"
"                      adjusting the game speed automatically,\n"
"                      depending on FPS. Useful in conjunction with\n"
"                      --ai_training."
msgstr " --auto_speed              Ann an geamannan ioma-chluicheadair a-mhàin,\n                           bidh seo a' cur gleus air luathas a' gheama\n                           gu fèin-obrachail a-rèir nam FPS. Tha seo\n                           feumail nuair a chleachdas tu\n                           --ai_training."

#: ../../src/wlapplication_messages.cc:111
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr " --scenario=AINM-FAIDHLE   Tòisichidh seo am mapa AINM-FAIDHLE sa bhad\n                           mar mhapa cnàmh-sgeòil."

#: ../../src/wlapplication_messages.cc:114
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=AINM-FAIDHLE   Luchdaichidh seo an geama air a shàbhaladh\n                           AINM-FAIDHLE sa bhad."

#: ../../src/wlapplication_messages.cc:115
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr " --script=AINM-FAIDHLE     Ruith an sgriobt Lua seo às dèidh an tòiseachaidh.\n                           Chan eil e dligheach ach le --scenario, --loadgame\n                           no --editor."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:121
msgid ""
" --auto_roadbuild_mode=[true|false]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr " --auto_roadbuild_mode=[true|false]\n                           Co-dhiù an dèid gus nach dèid do chur sa mhodh\n                           togail rathaidean gu fèin-obrachail nuair a bhios\n                           bratach air a chur ann nach eil ceangailte ri rathad."

#: ../../src/wlapplication_messages.cc:127
msgid "Graphic options:"
msgstr "Roghainnean grafaigeachd:"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:132
msgid ""
" --fullscreen=[true|false]\n"
"                      Whether to use the whole display for the\n"
"                      game screen."
msgstr " --fullscreen=[true|false]\n                           Co-dhiù an dèid gus nach dèid an sgrìn slàn a\n                           chleachdadh airson a' gheama."

#: ../../src/wlapplication_messages.cc:136
msgid " --xres=[...]         Width of the window in pixel."
msgstr " --xres=[...]              Leud na h-uinneige ann am piogsailean."

#: ../../src/wlapplication_messages.cc:137
msgid " --yres=[...]         Height of the window in pixel."
msgstr " --yres=[...]              Àirde na h-uinneige ann am piogsailean."

#: ../../src/wlapplication_messages.cc:138
msgid " --maxfps=[5 ...]     Maximal optical framerate of the game."
msgstr " --maxfps=[5 ...]          Reat as motha nam frèamaichean lèirsinneach sa gheama."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:143
msgid "Options for the internal window manager:"
msgstr "Roghainnean airson manaidsear nan uinneagan taobh a-staigh:"

#: ../../src/wlapplication_messages.cc:144
msgid ""
" --animate_map_panning=[yes|no]\n"
"                      Should automatic map movements be animated."
msgstr " --animate_map_panning=[yes|no]\n                           Am bi beòthachadh air gluasadan fèin-obrachail a’ mhapa\n                           gus nach bi."

#: ../../src/wlapplication_messages.cc:147
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr " --border_snap_distance=[0 ...]\n                           Gluais uinneag gu oir na sgrìn ma bhios i taobh\n                           a-staigh an astair seo o oir na sgrìn."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the
#. widelands
#. textdomain
#: ../../src/wlapplication_messages.cc:155
msgid ""
" --dock_windows_to_edges=[true|false]\n"
"                      Eliminate a window's border towards the\n"
"                      edge of the screen when the edge of the\n"
"                      window is next to the edge of the screen."
msgstr " --dock_windows_to_edges=[true|false]\n                           Thoir oir na h-uinneige ri taobh oir na sgrìn\n                           air falbh nuair a bhios oir na h-uinneige ri oir\n                           na sgrìn."

#: ../../src/wlapplication_messages.cc:160
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr " --panel_snap_distance=[0 ...]\n                           Gluais uinneag gu oir na panail ma bhios i taobh\n                           a-staigh an astair seo o oir na panail."

#: ../../src/wlapplication_messages.cc:165
msgid ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Only move a window to the edge of a panel\n"
"                      if the window is overlapping with the\n"
"                      panel."
msgstr " --snap_windows_only_when_overlapping=[yes|no]\n                           Na gluais uinneag gu oir panail ma bhios i\n                           tar-chòmdaichte leis a' phanail."

#: ../../src/wlapplication_messages.cc:171
msgid " --verbose            Enable verbose debug messages"
msgstr " --verbose                 Cuir teachdaireachdan dì-bhugachaidh briathrach\n                           an comas"

#: ../../src/wlapplication_messages.cc:172
msgid " --help               Show this help"
msgstr " --help                    Seall a' chobhair seo"

#: ../../src/wlapplication_messages.cc:173
msgid ""
"Bug reports? Suggestions? Check out the project website:\n"
"        https://www.widelands.org/\n"
"\n"
"Hope you enjoy this game!"
msgstr "A bheil thu airson innse dhuinn mu bhuga? No beachd a chur thugainn? Thoir sùil air làrach-lìn a' phròiseict:\n        https://www.widelands.org/\n\nTha sinn an dòchas gun còrd an geama riut!"
