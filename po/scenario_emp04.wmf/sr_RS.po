msgctxt "empire_building"
msgid "No soldier found for this training level!"
msgstr ""

#: ../../data/campaigns/emp04.wmf/scripting/tribes/well1/init.lua:7
msgctxt "empire_building"
msgid "Well"
msgstr ""
