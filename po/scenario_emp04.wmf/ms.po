#, lua-format
msgid "“%s”"
msgstr ""

#: ../../data/scripting/richtext_scenarios.lua:91
msgid "New Objective"
msgstr ""

#: ../../data/scripting/richtext_scenarios.lua:93
msgid "New Objectives"
msgstr ""
