msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=FILENAME  Charge directement la sauvegarde FILENAME."

#: ../../src/wlapplication.cc:1211
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr ""
" --script=FILENAME    Exécute le script LUA donné après l'initialisation.\n"
"                      Seulement valide avec --scenario, --loadgame, ou --"
"editor."

#: ../../src/wlapplication.cc:1213
msgid " --dedicated=FILENAME Starts a dedicated server with FILENAME as map"
msgstr ""
" --dedicated=FILENAME Démarre un serveur dédié avec FILENAME comme carte"

