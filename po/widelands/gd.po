msgid ""
" --remove_replays=[...]\n"
"                      Remove replays after this number of days.\n"
"                      If this is 0, replays are not deleted."
msgstr ""
" --remove_replays=[...]    Thoir air falbh na h-ath-chluichean às dèidh an\n"
"                           àireimh de làithean seo.\n"
"                           Cha tèid faidhlichean a sguabadh às mas e 0 a th' "
"ann."

#: ../../src/wlapplication.cc:1198
msgid "Sound options:"
msgstr "Roghainnean fuaime:"

#: ../../src/wlapplication.cc:1199
msgid " --nosound            Starts the game with sound disabled."
msgstr ""
" --nosound                 Tòisichidh seo an geama leis an fuaim à comas."

#: ../../src/wlapplication.cc:1200
msgid " --disable_fx         Disable sound effects."
msgstr " --disable_fx              Cuir èifeachdan fuaime à comas."

#: ../../src/wlapplication.cc:1201
msgid " --disable_music      Disable music."
msgstr " --disable_music           Cuir an ceòl à comas."

#: ../../src/wlapplication.cc:1202
msgid " --nozip              Do not save files as binary zip archives."
msgstr ""
" --nozip                   Na sàbhail faidhlichean nan tasglannan bìnearaidh "
"zip."

#: ../../src/wlapplication.cc:1203
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr ""
" --editor                  Tòisichidh seo an deasaichee Widelands sa bhad.\n"
"                           'S urrainn dhut =AINM-FAIDHLE a chur ris\n"
"                           gus am mapa AINM-FAIDHLE a luchdadh sa bhad\n"
"                           san deasaiche."

#: ../../src/wlapplication.cc:1206
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr ""
" --scenario=FILENAME       Tòisichidh seo am mapa AINM-FAIDHLE sa bhad\n"
"                           mar mhapa cnàmh-sgeòil."

#: ../../src/wlapplication.cc:1208
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr ""
" --loadgame=FILENAME       Luchdaichidh seo an geama air a shàbhaladh AINM-"
"FAIDHLE\n"
"                           sa bhad."

#: ../../src/wlapplication.cc:1209
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr ""
" --script=FILENAME         Ruith an sgriobt Lua seo às dèidh an "
"tòiseachaidh. Chan eil\n"
"                           e dligheach ach le --scenario, --loadgame, no --"
"editor."

#: ../../src/wlapplication.cc:1211
msgid " --dedicated=FILENAME Starts a dedicated server with FILENAME as map"
msgstr ""
" --dedicated=FILENAME      Tòisichidh seo frithealaiche sònraichte le AINM-"
"FAIDHLE\n"
"                           mar mhapa"

#: ../../src/wlapplication.cc:1212
msgid ""
" --auto_roadbuild_mode=[yes|no]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr ""
" --auto_roadbuild_mode=[yes|no]\n"
"                           Co-dhiù an tèid gus nach tèid do chur sa mhodh\n"
"                           togail rathaidean gu fèin-obrachail nuair a "
"bhios\n"
"                           bratach air a chur ann nach eil ceangailte ri "
"rathad."

