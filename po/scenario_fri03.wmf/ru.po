msgctxt "frisians_worker"
msgid "Diker"
msgstr "Ныряльщик"

#: ../../data/campaigns/fri03.wmf/scripting/tribes/dikers_house/init.lua:7
msgctxt "frisians_building"
msgid "Diker’s House"
msgstr "Домик ныряльщика"

#. TRANSLATORS: Completed/Skipped/Did not start diking because ...
#: ../../data/campaigns/fri03.wmf/scripting/tribes/dikers_house/init.lua:55
msgid "diking"
msgstr "погузился"

