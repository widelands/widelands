#, lua-format
msgid "“%s”"
msgstr "“%s”"

#: ../../data/scripting/format_scenario.lua:40
msgid "New Objective"
msgid_plural "New Objectives"
msgstr[0] "Nouveaux Objectif"
msgstr[1] "Nouveaux Objectifs"
