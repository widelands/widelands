#: ../../data/tribes/initialization/frisians/units.lua:1149
msgid "This berry bush is growing."
msgstr "Denne bærbusk vokser."

#. TRANSLATORS: Helptext for a frisian immovable: One of the berry bushes
#: ../../data/tribes/initialization/frisians/units.lua:932
#: ../../data/tribes/initialization/frisians/units.lua:960
#: ../../data/tribes/initialization/frisians/units.lua:988
