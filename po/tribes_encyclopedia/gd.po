msgid "This berry bush is growing."
msgstr ""

#. TRANSLATORS: Helptext for a frisian immovable: One of the berry bushes
#: ../../data/tribes/initialization/frisians/units.lua:913
#: ../../data/tribes/initialization/frisians/units.lua:941
#: ../../data/tribes/initialization/frisians/units.lua:969
