msgid "This berry bush is growing."
msgstr ""

#. TRANSLATORS: Helptext for a frisian immovable: One of the berry bushes
#: ../../data/tribes/initialization/frisians/units.lua:912
#: ../../data/tribes/initialization/frisians/units.lua:940
