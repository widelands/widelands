#: ../../data/tribes/initialization/frisians/units.lua:1114
#: ../../data/tribes/initialization/frisians/units.lua:1142
msgid "This berry bush has just been planted."
msgstr "Frissen ültetett bokor."

#. TRANSLATORS: Helptext for a frisian immovable: One of the berry bushes
#: ../../data/tribes/initialization/frisians/units.lua:925
#: ../../data/tribes/initialization/frisians/units.lua:953
#: ../../data/tribes/initialization/frisians/units.lua:981
#: ../../data/tribes/initialization/frisians/units.lua:1009
#: ../../data/tribes/initialization/frisians/units.lua:1037
