#: ../../data/tribes/initialization/empire/units.lua:2254
#: ../../data/tribes/initialization/frisians/units.lua:2161
msgctxt "building"
msgid "‘Don’t swear at the builder who is short of building materials.’"
msgstr "„Ne szidd az építőmunkást azért, mert nincs építőanyaga!”"

