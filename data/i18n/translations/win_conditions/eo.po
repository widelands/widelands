#, lua-format
msgid "%i artifact"
msgid_plural "%i artifacts"
msgstr[0] "%i artefakto"
msgstr[1] "%i artefaktoj"

#. TRANSLATORS: e.g. Team 1 owns 5 artifacts.
#: ../../../../data/scripting/win_conditions/artifacts.lua:184
msgid "%1$s owns %2$s."
msgstr "%1$s posedas %2$s. "

