msgstr[1] "%1% jednotky %2% boli prenesené do vašej ‘%3%’ budovy."
msgstr[2] "%1% jednotiek %2% bolo prenesených do vašej ‘%3%’ budovy."
msgstr[3] "%1% jednotky %2% bola prenesených do vašej ‘%3%’ budovy."

#: ../../../../data/maps/SP_Scenarios/Along_The_River.wmf/scripting/functions.lua:60
#: ../../../../data/maps/SP_Scenarios/Along_The_River.wmf/scripting/functions.lua:62
